{
  "name": "frontend",
  "version": "0.0.13",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "yarn type-check &&vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite --port 4173' http://localhost:4173 'cypress open --e2e'",
    "cypress:open": "npx cypress open --e2e --config baseUrl=http://localhost:5173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . ",
    "lint:fix": "eslint .  --fix ",
    "format": "prettier --cache --check src/",
    "format:fix": "prettier --cache --check src/ --write",
    "lintformat:fix": "yarn lint:fix && yarn format:fix",
    "depcheck": "depcheck",
    "update-version": "npm version patch --no-git-tag-version && yarn install --silent && node ./scripts/create-branch.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@sentry/vue": "^10.12.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@vue-leaflet/vue-leaflet": "^0.10.1",
    "@vuepic/vue-datepicker": "^11.0.2",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.7.9",
    "chart.js": "^4.4.7",
    "jszip": "^3.10.1",
    "keycloak-js": "^26.0.7",
    "leaflet": "^1.9.4",
    "leaflet-geosearch": "^4.0.0",
    "pinia": "^3.0.3",
    "qr-code-styling": "^1.8.4",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.12",
    "vue-leaflet-markercluster": "^0.6.1",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.19.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/jsdom": "^21.1.7",
    "@types/leaflet": "^1.9.15",
    "@types/node": "^24.5.1",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.20",
    "cypress": "^15.2.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.20.0",
    "eslint-plugin-cypress": "^5.1.1",
    "eslint-plugin-vue": "^10.4.0",
    "jsdom": "^27.0.0",
    "npm": "^11.1.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "sass": "^1.82.0",
    "start-server-and-test": "^2.0.9",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.9.2",
    "vite": "^7.1.5",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.7"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
